.ratingbar-stars-s1, .ratingbar-stars-s2, .ratingbar-stars-s3 {
  display: flex;
  flex-direction: row;
  justify-content: space-around;
  align-items: center;
}

/* class="ratingbar-stars"的控件下所包含的image控件的样式 */

.ratingbar-stars-s1 image {
  height: 26rpx;
  width: 26rpx;
  margin-right: 8rpx;
}

.ratingbar-stars-s2 image {
  height: 37rpx;
  width: 26rpx;
}

.ratingbar-stars-s3 image {
  height: 34rpx;
  width: 34rpx;
}

/* 关于注册弹窗 */

.black-mask {
  position: fixed;
  left: 0;
  top: 0;
  z-index: 1111111;
  width: 100vw;
  height: 100vh;
  background-color: rgba(0, 0, 0, 0.7);
  align-items: center;
  justify-content: center;
}

.style-con {
  width: 650rpx;
  height: 610rpx;
  background-color: #eee;
  border-radius: 20rpx;
  position: relative;
  align-items: center;
}

.titleimg {
  width: 102rpx;
  height: 53rpx;
  margin-top: 36rpx;
}

.close-btn {
  position: absolute;
  top: 24rpx;
  right: 25rpx;
  width: 54rpx;
  height: 54rpx;
}

.style1-box {
  width: 557rpx;
  height: 350rpx;
  background-color: #fff;
  border-radius: 10rpx;
  align-items: center;
  justify-content: space-around;
  margin-top: 22rpx;
  margin-bottom: 34rpx;
}

.up-box {
  width: 100%;
  height: 40rpx;
  align-items: center;
  justify-content: center;
}

.up1 {
  font-size: 38rpx;
  font-family: PingFangSC-Medium;
}

.up2 {
  font-size: 38rpx;
  font-family: PingFangSC-Medium;
