}
.mid-box{
  width: 100%;
  height: 54rpx;
  align-items: center;
  justify-content: center;
}
.mid-box>image{
  width: 54rpx;
  height: 54rpx;
  margin-left: 25rpx;
}
.mid1{
  font-size:72rpx;
  font-family:PingFangSC-Medium;
}
.down-box{
  width: 460rpx;
  line-height: 40rpx;
  height: 80rpx;
  font-size: 28rpx;
  color: #505050;
  text-align: center;
}
.down-box>image{
  width: 26rpx;
  height: 26rpx;
}
.confirmBTN {
  width: 260rpx;
  height: 78rpx;
  background-color: #f7cd47;
  border-radius: 39rpx;
  position: relative;
  font-size: 30rpx;
  font-family: PingFangSC-Regular;
  line-height: 78rpx;
  text-align: center;
}
