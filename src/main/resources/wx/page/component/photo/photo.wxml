<!--index.wxml-->
<view class="main">
  <view class="form-box">
    <form bindsubmit="formSubmit">
            <input placeholder="名字" name="name"/>
            <image class="image" src="{{imageObject.imageURL}}" mode="aspectFit"></image>
            <button bindtap='didPressChooesImage'>上传图片</button>
            <input placeholder="url" name="url" value='{{imageObject.imageURL}}'></input>
            <input placeholder="描述" name="other"/>
            <input placeholder="创建时间" name="createDate"/>
            <button form-type="submit">保存</button>
    </form>
  </view>
</view>
